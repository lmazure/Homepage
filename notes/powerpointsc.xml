<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="../css/strict.xsl"?>
<PAGE xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../css/schema.xsd">
<TITLE>French PowerPoint shortcuts</TITLE>
<PATH>notes/powerpointsc.xml</PATH>
<DATE><YEAR>2015</YEAR><MONTH>4</MONTH><DAY>18</DAY></DATE>
<CONTENT>
General
<DEFINITIONTABLE>
<ROW><TERM>
<KEY id="F5"/>
</TERM><DESC>
slide show mode
</DESC></ROW>
</DEFINITIONTABLE><BR/>
Navigate
<DEFINITIONTABLE>
<ROW><TERM>
<KEY id="F6"/>
</TERM><DESC>
go to next pane
</DESC></ROW>
<ROW><TERM>
<MODIFIERKEY id="Shift"/><KEY id="F6"/>
</TERM><DESC>
go to previous pane
</DESC></ROW>
<ROW><TERM>
<MODIFIERKEY id="Ctrl"/><KEY id="F6"/>
</TERM><DESC>
go to next document
</DESC></ROW>
<ROW><TERM>
<MODIFIERKEY id="Ctrl"/><MODIFIERKEY id="Shift"/><KEY id="F6"/>
</TERM><DESC>
go to previous document
</DESC></ROW>
</DEFINITIONTABLE><BR/>
Edit
<DEFINITIONTABLE>
<ROW><TERM>
<KEY id="F2"/>
</TERM><DESC>
toggle select the whole text - select the object
</DESC></ROW>
<ROW><TERM>
<MODIFIERKEY id="Ctrl"/><KEY id="M"/>
</TERM><DESC>
insert a slide
</DESC></ROW>
<ROW><TERM>
<MODIFIERKEY id="Ctrl"/><KEY id="F"/>
</TERM><DESC>
find
</DESC></ROW>
<ROW><TERM>
<MODIFIERKEY id="Ctrl"/><KEY id="H"/>
</TERM><DESC>
replace
</DESC></ROW>
<ROW><TERM>
<MODIFIERKEY id="Alt"/><MODIFIERKEY id="Shift"/><KEY id="Left"/>
</TERM><DESC>
promote
</DESC></ROW>
<ROW><TERM>
<MODIFIERKEY id="Alt"/><MODIFIERKEY id="Shift"/><KEY id="Right"/>
</TERM><DESC>
demote
</DESC></ROW>
<ROW><TERM>
<MODIFIERKEY id="Ctrl"/><MODIFIERKEY id="Shift"/><KEY id="+"/>
</TERM><DESC>
set the characters to superscript
</DESC></ROW>
<ROW><TERM>
<MODIFIERKEY id="Shift"/><KEY id="F3"/>
</TERM><DESC>
change the case of the selection (lower / first upper / upper)
</DESC></ROW>
<ROW><TERM>
<MODIFIERKEY id="Ctrl"/><KEY id="F9"/>
</TERM><DESC>
show/hide grid
</DESC></ROW>
<ROW><TERM>
<MODIFIERKEY id="Alt"/><KEY id="F9"/>
</TERM><DESC>
show/hide guide lines
</DESC></ROW>
<ROW><TERM>
<MODIFIERKEY id="Ctrl"/><KEY id="F2"/>
</TERM><DESC>
print preview
</DESC></ROW>
</DEFINITIONTABLE><BR/>
Slide Show Mode
<DEFINITIONTABLE>
<ROW><TERM>
<KEY id="N"/><BR/>
<KEY id="Enter"/><BR/>
<KEY id="PageDown"/><BR/>
<KEY id="Right"/><BR/>
<KEY id="Down"/><BR/>
<KEY id="Space"/><BR/>
mouse click
</TERM><DESC>
next slide or animation
</DESC></ROW>
<ROW><TERM>
<KEY id="P"/><BR/>
<KEY id="PageUp"/><BR/>
<KEY id="Left"/><BR/>
<KEY id="Up"/><BR/>
<KEY id="Backspace"/><BR/>
</TERM><DESC>
previous slide or animation
</DESC></ROW>
<ROW><TERM>
&lt;number&gt; <KEY id="Enter"/>
</TERM><DESC>
go to slide &lt;number&gt;
</DESC></ROW>
<ROW><TERM>
<KEY id="B"/><BR/>
<KEY id="."/>
</TERM><DESC>
toggle black screen on/off
</DESC></ROW>
<ROW><TERM>
<KEY id="W"/><BR/>
<KEY id=","/>
</TERM><DESC>
toggle white screen on/off
</DESC></ROW>
<ROW><TERM>
<KEY id="S"/><BR/>
<KEY id="+"/>
</TERM><DESC>
stop/restart automatic slide show
</DESC></ROW>
<ROW><TERM>
<KEY id="Esc"/><BR/>
<MODIFIERKEY id="Ctrl"/><KEY id="Break"/><BR/>
<KEY id="-"/>
</TERM><DESC>
end of slide show
</DESC></ROW>
<ROW><TERM>
<KEY id="E"/>
</TERM><DESC>
erase annotations
</DESC></ROW>
<ROW><TERM>
<MODIFIERKEY id="Ctrl"/><KEY id="P"/>
</TERM><DESC>
display on/off pen pointer
</DESC></ROW>
<ROW><TERM>
<MODIFIERKEY id="Ctrl"/><KEY id="A"/>
</TERM><DESC>
display on/off arrow pointer
</DESC></ROW>
<ROW><TERM>
<MODIFIERKEY id="Ctrl"/><KEY id="H"/>
</TERM><DESC>
hide pointer and button
</DESC></ROW>
</DEFINITIONTABLE><BR/>
</CONTENT>
</PAGE>