<U><B>Gmail</B></U>
<DEFINITIONTABLE>
  <ROW>
    <TERM><MODIFIERKEY id="Shift"/><KEY id=","/></TERM>
    <DESC>Display keyboard shortcuts</DESC>
  </ROW>
  <ROW>
    <TERM><KEY id="G"/><KEY id="I"/></TERM>
    <DESC>Display Inbox</DESC>
  </ROW>
  <ROW>
    <TERM><KEY id="G"/><KEY id="S"/></TERM>
    <DESC>Display Starred messages</DESC>
  </ROW>
  <ROW>
    <TERM><KEY id="G"/><KEY id="B"/></TERM>
    <DESC>Display Snoozed messages</DESC>
  </ROW>
  <ROW>
    <TERM><KEY id="G"/><KEY id="T"/></TERM>
    <DESC>Display Sent messages</DESC>
  </ROW>
  <ROW>
    <TERM><KEY id="G"/><KEY id="D"/></TERM>
    <DESC>Display Draft messages</DESC>
  </ROW>
  <ROW>
    <TERM><KEY id="G"/><KEY id="A"/></TERM>
    <DESC>Display All messages</DESC>
  </ROW>
  <ROW>
    <TERM><KEY id="G"/><KEY id="E"/></TERM>
    <DESC>Display contacts</DESC>
  </ROW>
  <ROW>
    <TERM><KEY id="G"/><KEY id="L"/></TERM>
    <DESC>Display labels for searching</DESC>
  </ROW>
  <ROW>
    <TERM><KEY id="K"/></TERM>
    <DESC>Display next (toward the future) mail thread</DESC>
  </ROW>
  <ROW>
    <TERM><KEY id="J"/></TERM>
    <DESC>Display previous (toward the past) mail thread</DESC>
  </ROW>
  <ROW>
    <TERM><KEY id="O"/><BR/>
      <KEY id="Enter"/></TERM>
    <DESC>Display mail thread</DESC>
  </ROW>
  <ROW>
    <TERM><KEY id="U"/></TERM>
    <DESC>Display the list of mail threads</DESC>
  </ROW>
  <ROW>
    <TERM><KEY id=";"/></TERM>
    <DESC>Expand entire conversation</DESC>
  </ROW>
  <ROW>
    <TERM><KEY id=":"/></TERM>
    <DESC>Collapse entire conversation</DESC>
  </ROW>
  <ROW>
    <TERM><KEY id="L"/></TERM>
    <DESC>Open "label as" menu</DESC>
  </ROW>
  <ROW>
    <TERM><KEY id="V"/></TERM>
    <DESC>Open "move" menu</DESC>
  </ROW>
  <ROW>
    <TERM><KEY id="C"/></TERM>
    <DESC>Create a new mail</DESC>
  </ROW>
  <ROW>
    <TERM><MODIFIERKEY id="Ctrl"/><MODIFIERKEY id="Alt"/><KEY id="3"/></TERM>
    <DESC>Delete message</DESC>
  </ROW>
  <ROW>
    <TERM><KEY id="E"/></TERM>
    <DESC>Archive (i.e. remove "Inbox" label)</DESC>
  </ROW>
  <ROW>
    <TERM><KEY id="Z"/></TERM>
    <DESC>Cancel last action</DESC>
  </ROW>
  <ROW>
    <TERM><KEY id="R"/></TERM>
    <DESC>Reply</DESC>
  </ROW>
  <ROW>
    <TERM><MODIFIERKEY id="Shift"/><KEY id="R"/></TERM>
    <DESC>Reply in new window</DESC>
  </ROW>
  <ROW>
    <TERM><KEY id="A"/></TERM>
    <DESC>Reply to all</DESC>
  </ROW>
  <ROW>
    <TERM><MODIFIERKEY id="Shift"/><KEY id="A"/></TERM>
    <DESC>Reply to all in new window</DESC>
  </ROW>
  <ROW>
    <TERM><KEY id="F"/></TERM>
    <DESC>Forward</DESC>
  </ROW>
  <ROW>
    <TERM><MODIFIERKEY id="Shift"/><KEY id="F"/></TERM>
    <DESC>Forward in new window</DESC>
  </ROW>
</DEFINITIONTABLE>
