<U><B>Hangouts</B></U>
<DEFINITIONTABLE>
  <ROW>
    <TERM><MODIFIERKEY id="Shift"/><KEY id="Return"/></TERM>
    <DESC>Add a newline without sending the message</DESC>
  </ROW>
  <ROW>
    <TERM><MODIFIERKEY id="Ctrl"/><KEY id="N"/></TERM>
    <DESC>Start a new conversation</DESC>
  </ROW>
</DEFINITIONTABLE>
