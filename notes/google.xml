<U><B>Google docs</B></U>
<DEFINITIONTABLE>
  <ROW>
    <TERM><MODIFIERKEY id="Alt"/><MODIFIERKEY id="Shift"/><KEY id="5"/></TERM>
    <DESC>Toggle strike-through</DESC>
  </ROW>
  <ROW>
    <TERM><MODIFIERKEY id="Ctrl"/><MODIFIERKEY id="Alt"/><KEY id="0"/></TERM>
    <DESC>Apply Normal Text style</DESC>
  </ROW>
  <ROW>
    <TERM><MODIFIERKEY id="Ctrl"/><MODIFIERKEY id="Alt"/><KEY id="1"/></TERM>
    <DESC>Apply Heading 1 style</DESC>
  </ROW>
  <ROW>
    <TERM><MODIFIERKEY id="Ctrl"/><MODIFIERKEY id="Alt"/><KEY id="2"/></TERM>
    <DESC>Apply Heading 2 style</DESC>
  </ROW>
  <ROW>
    <TERM><MODIFIERKEY id="Ctrl"/><MODIFIERKEY id="Alt"/><KEY id="3"/></TERM>
    <DESC>Apply Heading 3 style</DESC>
  </ROW>
  <ROW>
    <TERM><MODIFIERKEY id="Ctrl"/><MODIFIERKEY id="Alt"/><KEY id="4"/></TERM>
    <DESC>Apply Heading 4 style</DESC>
  </ROW>
  <ROW>
    <TERM><MODIFIERKEY id="Ctrl"/><MODIFIERKEY id="Alt"/><KEY id="5"/></TERM>
    <DESC>Apply Heading 5 style</DESC>
  </ROW>
  <ROW>
    <TERM><MODIFIERKEY id="Ctrl"/><MODIFIERKEY id="Alt"/><KEY id="6"/></TERM>
    <DESC>Apply Heading 6 style</DESC>
  </ROW>
