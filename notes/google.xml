    <TERM><MODIFIERKEY id="Alt"/><MODIFIERKEY id="Shift"/><KEY id="Up"/></TERM>
    <DESC>Move the selected lines upwards</DESC>
  </ROW>
  <ROW>
    <TERM><MODIFIERKEY id="Alt"/><MODIFIERKEY id="Shift"/><KEY id="Down"/></TERM>
    <DESC>Move the selected lines downwards</DESC>
