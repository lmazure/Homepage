<BLIST><TITLE>CLI</TITLE>
  <ITEM><SLIST><TITLE>Print command line help</TITLE>
    <ITEM><CODEROUTINE>Code.exe --help</CODEROUTINE></ITEM>
  </SLIST></ITEM>
  <ITEM><SLIST><TITLE>Print version</TITLE>
    <ITEM><CODEROUTINE>Code.exe --version</CODEROUTINE></ITEM>
  </SLIST></ITEM>
  <ITEM><SLIST><TITLE>Wait for the files to be closed before returning</TITLE>
    <ITEM><CODEROUTINE>Code.exe --wait file1 file2</CODEROUTINE></ITEM>
  </SLIST></ITEM>
  <ITEM><SLIST><TITLE>Open file at given position</TITLE>
    <ITEM><CODEROUTINE>Code.exe --wait file1:line</CODEROUTINE></ITEM>
    <ITEM><CODEROUTINE>Code.exe --wait file1:line:column</CODEROUTINE></ITEM>
  </SLIST></ITEM>
  <ITEM><SLIST><TITLE>Specify the directory that user data is kept in</TITLE>
    <ITEM><CODEROUTINE>--user-data-dir H:\Documents\tools\VSCode\myDataDir</CODEROUTINE></ITEM>
  </SLIST></ITEM>
  <ITEM><SLIST><TITLE>Specify the directory where the extensions are installed</TITLE>
    <ITEM><CODEROUTINE>--extensions-dir H:\Documents\tools\VSCode\extensions</CODEROUTINE></ITEM>
  </SLIST></ITEM>
</BLIST>
