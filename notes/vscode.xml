  </ROW>
  <ROW>
    <TERM><MODIFIERKEY id="Ctrl"/><MODIFIERKEY id="Shift"/><KEY id="H"/></TERM>
    <DESC>replace in files</DESC>
  </ROW>
</DEFINITIONTABLE>
